{
  "name": "discord-assistant",
  "version": "1.0.0",
  "description": "A fun little bot made with Javascript with all the features you wanted as a Discord Moderator! It's got Moderation, Fun commands, Welcomer and many more stuff for free!",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/src/index.js",
    "start:dev": "nodemon ./build/src/index.js",
    "build": "tsc -p tsconfig.json",
    "build:dev": "tsc -p tsconfig.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackarmour/discord-assistant-js.git"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hackarmour/discord-assistant-js/issues"
  },
  "homepage": "https://github.com/hackarmour/discord-assistant-js#readme",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "axios": "^0.24.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.1",
    "mongoose": "^6.1.5"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  }
